{
  "name": "portfolio",
  "version": "0.1.0",
  "homepage": "https://Sukanth-R.github.io/my-portfolio",
  "private": true,
  "dependencies": {
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@tsparticles/all": "^3.8.1",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.8.1",
    "axios": "^1.7.9",
    "cra-template": "1.2.0",
    "framer-motion": "^6.5.1",
    "lucide-react": "^0.469.0",
    "mongoose": "^8.9.3",
    "next": "^15.1.6",
    "nodemailer": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-feather": "^2.0.10",
    "react-hot-toast": "^2.5.1",
    "react-hover-animation": "^2.1.1",
    "react-icons": "^5.4.0",
    "react-intersection-observer": "^9.15.1",
    "react-router-dom": "^7.1.5",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.9.3",
    "react-typing-effect": "^2.0.5",
    "styled-components": "^6.1.14",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --ci",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write src/**/*.{js,jsx,json,css,scss}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.3.0",
    "jest-environment-jsdom": "^29.0.0",
    "jest-junit": "^15.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.17"
  },
  "jest": {
    "reporters": [
      "default",
      ["jest-junit", {
        "outputDirectory": "./test-results",
        "outputName": "junit.xml"
      }]
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/src/setupTests.js"],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios|@tsparticles|react-hover-animation)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/setupTests.js",
      "/src/reportWebVitals.js"
    ],
    "collectCoverage": true,
    "coverageReporters": ["lcov", "text", "json"],
    "reporters": [
      "default",
      ["jest-junit", { "outputDirectory": "test-results" }]
    ]
  }
}
